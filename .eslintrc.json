{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      { "argsIgnorePattern": "^_" }
    ],
    "no-warning-comments": [
      "warn",
      { "terms": ["todo", "fixme"], "location": "anywhere" }
    ],
    "no-console": "off"
  }
}
